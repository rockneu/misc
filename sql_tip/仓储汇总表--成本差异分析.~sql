
/*
SELECT * --NVL(SUM(T.TRX_QTY),0) QTY,NVL(SUM(T.AMOUNT),0) AMOUNT
--      INTO L_ITEM_QTY_LEVEL2_BEGIN_B ,L_ITEM_AMOUNT_LEVEL2_BEGIN_B
      FROM CUX_INV_TRX_DETAILS_V T--CUX_INV_TRX_DETAILS_TEMP T
     WHERE T.Organization_Id = 102
       AND TO_CHAR(T.TRANSACTION_DATE,'YYYY-MM-DD HH24:MI:SS') <= '2014-11-23 23:59:59'--L_DATE_FROM
       AND t.trx_type IN ( '二级库其他','二级库到二级库','二级库发料','二级库发送一级库','二级库接收一级库')
       ;      
       */
       
SELECT T.TRANSACTION_ID, TO_CHAR(T.CREATION_DATE,'YYYY-MM-DD HH24:MI:SS'),TO_CHAR(T.TRANSACTION_DATE,'YYYY-MM-DD HH24:MI:SS'), 
       T.ORGANIZATION_ID, T.SUBINVENTORY_CODE,T.TRANSACTION_SOURCE_NAME, T.SOURCE_CODE,T.ATTRIBUTE1,
       T.TRANSACTION_QUANTITY, ROUND(T.ACTUAL_COST,3), ROUND(T.TRANSACTION_QUANTITY * T.ACTUAL_COST,2)
FROM mtl_material_transactions T
WHERE T.ORGANIZATION_ID=969
/*      AND TO_CHAR(T.TRANSACTION_DATE,'YYYY-MM-DD HH24:MI:SS') <= '2014-11-23 23:59:59'
      AND TO_CHAR(T.creation_DATE,'YYYY-MM-DD HH24:MI:SS') > '2014-11-23 23:59:59'*/
       AND TO_CHAR(T.TRANSACTION_DATE,'YYYY-MM-DD HH24:MI:SS') <= '2014-12-24 23:59:59'
      AND TO_CHAR(T.creation_DATE,'YYYY-MM-DD HH24:MI:SS') > '2014-12-24 23:59:59'
;       


