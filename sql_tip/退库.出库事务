
-- data ? TK201409180001, 1274981
SELECT ATTRIBUTE1,ATTRIBUTE2,tRANSACTION_SOURCE_NAME ,mt.* 
FROM Mtl_Material_Transactions MT --WHERE SUBSTR(ATTRIBUTE1,1,2) IN ('CK' ,'TK') ;
--WHERE ATTRIBUTE1 IN ('' ,'TK201409180001') ;
WHERE ATTRIBUTE2 IN ('' ,'1324624');


SELECT ATTRIBUTE1,ATTRIBUTE2,tRANSACTION_SOURCE_NAME ,mt.* FROM Mtl_Material_Transactions MT --WHERE SUBSTR(ATTRIBUTE1,1,2) IN ('CK' ,'TK') ;
--WHERE ATTRIBUTE1 IN ('' ,'TK201409180001') ;
WHERE tRANSACTION_SOURCE_NAME IN ('' ,'WO:445474') ;

--TK201409180002
SELECT ATTRIBUTE1,ATTRIBUTE2,tRANSACTION_SOURCE_NAME ,mt.* FROM Mtl_Material_Transactions MT 
--WHERE ATTRIBUTE1 IN ('' ,'TK201409180002') ;
WHERE ATTRIBUTE2 IN ('' ,'LY01201409180007') ;

SELECT ATTRIBUTE1,ATTRIBUTE2,tRANSACTION_SOURCE_NAME ,mt.* 
FROM Mtl_Material_Transactions  MT
WHERE TRANSACTION_SOURCE_NAME IN (SELECT tRANSACTION_SOURCE_NAME FROM Mtl_Material_Transactions
                                            WHERE tRANSACTION_SOURCE_NAME IS NOT NULL AND ATTRIBUTE1 LIKE 'TK%')
ORDER BY MT.CREATION_DATE DESC, MT.tRANSACTION_SOURCE_NAME, MT.ATTRIBUTE1;

